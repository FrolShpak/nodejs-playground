{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "object-curly-spacing": ["error", "always"],
        "indent": ["error", 4],
        "new-cap": "warn",
        "no-unused-vars": "warn",
        "max-len": "warn"
    }
}
